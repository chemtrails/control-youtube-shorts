{
    "manifest_version": 2,
    "name": "shorts",
    "description": "shorts",
    "version": "0.2",
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "content.css"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}